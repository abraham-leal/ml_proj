              value: "internal://"
            - name: TESTING
              value: "true"
            
          livenessProbe:
            httpGet:
              path: /console/api/healthz
              port: http
          readinessProbe:
            httpGet:
              path: /console/api/ready
              port: http
            initialDelaySeconds: 20
            periodSeconds: 5
          startupProbe:
            httpGet:
              path: /console/api/ready
              port: http
            initialDelaySeconds: 20
            periodSeconds: 5
            failureThreshold: 120
          # Increase the sleep before SIGTERM to 25s. I had this as 5s previously and it wasn't enough.
