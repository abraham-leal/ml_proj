              valueFrom:
                secretKeyRef:
                  name: wandb-mysql
                  key: MYSQL_ROOT_PASSWORD
            - name: MYSQL_PASSWORD
              valueFrom:
                secretKeyRef:
                  name: wandb-mysql
                  key: MYSQL_PASSWORD
            
            - name: BUCKET_QUEUE
              value: "internal://"
            
          livenessProbe:
            exec:
              command:
              - sh
              - -c
              - "mysqladmin ping -u root -p${MYSQL_ROOT_PASSWORD}"
          readinessProbe:
            exec:
              command:
              - sh
              - -c
              - "mysqladmin ping -u root -p${MYSQL_ROOT_PASSWORD}"
          volumeMounts:
            - name: data
              mountPath: /var/lib/mysql
            - name: initdb
              mountPath: /docker-entrypoint-initdb.d/initdb.sql
              subPath: initdb.sql
            - name: initdb
              mountPath: /etc/mysql/my.cnf
              subPath: my.cnf
          resources:
            limits:
              cpu: 4000m
              memory: 8Gi
            requests:
              cpu: 500m
              memory: 1Gi
      
      volumes:
        - name: initdb
          configMap:
            name: wandb-mysql-initdb
        - name: data
          persistentVolumeClaim:
            claimName: wandb-mysql-data
